<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

   <!--  Bootstraps the web client configuration -->
   <bean id="ad.extension.webClientBootstrap" class="org.alfresco.web.config.WebClientConfigBootstrap" init-method="init">
      <property name="configs">
         <list>
            <value>classpath:alfresco/extension/web-client-config-adempiere-custom.xml</value>
         </list>
       </property>
  </bean> 
  
</beans>  